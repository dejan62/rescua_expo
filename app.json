{
  "expo": {
    "name": "RESCUA rescuapp",
    "slug": "rescuapp",
    "version": "0.9.2",
    "orientation": "portrait",
    "icon": "./assets/images/LOGO_BLUE_SQUARE.png",
    "scheme": "rescuapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/LOGO_BLUE_SQUARE.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rescua.rescuapp",
      "buildNumber": "9",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Allow Rescuapp to use your location for navigation and emergency features.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Allow Rescuapp to use your location for emergency features.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/LOGO_BLUE_SQUARE.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rescua.rescuapp",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "softwareKeyboardLayoutMode": "resize",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "com.rescua",
              "host": "com.rescua",
              "pathPrefix": "/android/callback"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cf58b712-c6ad-4e68-bc47-cb10d81d47bf"
      }
    }
  }
}
